{
  "name": "react-rwd-slideshow",
  "version": "1.0.12",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-terser": "^0.2.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.8.1",
    "rollup": "^2.79.1"
  },
  "description": "React RWD Slideshow, no dependencies",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/engdlee/react-rwd-slideshow.git"
  },
  "keywords": [
    "slideshow",
    "react-slideshow",
    "react-component",
    "RWD",
    "carousel",
    "gallery",
    "slider",
    "responsive",
    "mobile",
    "tablet",
    "desktop",
    "touch"
  ],
  "author": "David Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/engdlee/react-rwd-slideshow/issues"
  }
}
